<%- include('../common/headers.ejs') %>
<link
  rel="stylesheet"
  href="/clientAnnouncementModule/announcement/announcement.css"
/>

<a href="/"><div class="header">&lt; Back</div></a>

<div class="header-2">Announcements</div>

<div class="announcement-container">
  <% for(const announcement of announcements) { %>
  <a href="/announcement/<%= announcement._id %>" style="display: contents">
    <div class="announcement-item">
      <div>ICON</div>
      <div><%= announcement.title %></div>
      <div>></div>
    </div>
  </a>
  <% } %>
</div>
